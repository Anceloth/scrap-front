# Development Environment Configuration
VITE_APP_NAME=React Auth App (Dev)
VITE_APP_VERSION=1.0.0-dev
VITE_APP_DESCRIPTION=Modern React Authentication Application - Development

# API Configuration - Development
VITE_API_BASE_URL=http://localhost:3001/api
VITE_API_TIMEOUT=5000

# Authentication Configuration
VITE_AUTH_TOKEN_KEY=dev_auth_token
VITE_AUTH_REFRESH_TOKEN_KEY=dev_refresh_token
VITE_AUTH_TOKEN_EXPIRY=7200000

# Theme Configuration
VITE_THEME_STORAGE_KEY=dev_theme_mode
VITE_DEFAULT_THEME=light

# Feature Flags - All enabled for development
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_PASSWORD_RESET=true
VITE_ENABLE_REMEMBER_ME=true

# Development Configuration
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Social Auth - Development
VITE_GOOGLE_CLIENT_ID=dev_google_client_id
VITE_FACEBOOK_APP_ID=dev_facebook_app_id
